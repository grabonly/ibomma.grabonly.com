---
import Content from "../components/Content.astro";
import Footer from "../components/Footer.astro";
import Header from "../components/Header.astro";
import Layout from "../layouts/Layout.astro";
import {title, desc} from "../API/defaultSiteValues.js"
import { YYYY, metaUpdatedTime } from "../API/time";
const year = YYYY();
---

<Layout title={title+" "+year+" - New Telugu, Tamil, Hindi Movies iBOMMA"} desc={desc} index={true}>
    <Header title={title} />
    <Content/>
    <Footer title={title} />
</Layout>